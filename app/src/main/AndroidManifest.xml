<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.enfotrix.studentportal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentPortal">
        <activity android:name=".Activities.ActivityAttendance"></activity>
        <activity android:name=".Activities.ActivityFeedback" />
        <activity android:name=".Activities.ActivityProgressReport" />
        <activity android:name=".Activities.ActivityGallery" />
        <activity android:name=".Activities.ActivityAnnouncement" />
        <activity android:name=".Activities.ActivityResetPassword" />
        <activity android:name=".Activities.ActivityForgetPasswordAuth" />
        <activity android:name=".Activities.ActivityLogin" />
        <activity android:name=".Activities.ActivitySplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MainActivity" />
    </application>

</manifest>